<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<mode>
						<TCEforms>
							<onChange>reload</onChange>
							<displayCond>EXT:dam:LOADED:true</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.mode</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<itemsProcFunc>tx_imagecycle_itemsProcFunc->getModes</itemsProcFunc>
							</config>
						</TCEforms>
					</mode>
					<images>
						<TCEforms>
							<displayCond>FIELD:mode:!IN:dam,dam_catedit</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.images</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>files</allowed>
								<size>12</size>
								<maxitems>1000</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<allowed>gif,jpg,jpeg,png</allowed>
								<max_size>10240</max_size>
								<uploadfolder>uploads/tx_imagecycle</uploadfolder>
							</config>
						</TCEforms>
					</images>
					<captions>
						<TCEforms>
							<displayCond>FIELD:mode:!IN:dam,dam_catedit</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.caption</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</captions>
					<damimages>
						<TCEforms>
							<displayCond>FIELD:mode:=:dam</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.damimages</label>
							<config>
								<type>group</type>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:dam/lib/class.tx_dam_tcefunc.php:&tx_dam_tceFunc->getSingleField_typeMedia]]></userFunc>
								<userProcessClass><![CDATA[EXT:mmforeign/class.tx_mmforeign_tce.php:tx_mmforeign_tce]]></userProcessClass>
								<internal_type>db</internal_type>
								<allowed>tx_dam</allowed>
								<allowed_types>gif,jpg,jpeg,png</allowed_types>
								<prepend_tname>1</prepend_tname>
								<MM>tx_dam_mm_ref</MM>
								<MM_foreign_select>1</MM_foreign_select>
								<MM_opposite_field>1</MM_opposite_field>
								<MM_match_fields>
									<ident>imagecycle</ident>
								</MM_match_fields>
								<show_thumbs>1</show_thumbs>
								<size>10</size>
								<maxitems>1000</maxitems>
								<minitems>0</minitems>
								<autoSizeMax>30</autoSizeMax>
							</config>
						</TCEforms>
					</damimages>
					<damcategories>
						<TCEforms>
							<displayCond>FIELD:mode:=:dam_catedit</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.damcategories</label>
							<config>
								<type>select</type>
								<form_type>user</form_type>
								<userFunc>EXT:dam/lib/class.tx_dam_tcefunc.php:tx_dam_tceFunc->getSingleField_selectTree</userFunc>
								<treeViewClass>EXT:dam/components/class.tx_dam_selectionCategory.php:tx_dam_selectionCategory</treeViewClass>
								<foreign_table>tx_dam_cat</foreign_table>
								<size>5</size>
								<autoSizeMax>25</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<default></default>
							</config>
						</TCEforms>
					</damcategories>
				</el>
			</ROOT>
		</general>
		<settings>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.sheet_settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<imagewidth>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.imagewidth</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</imagewidth>
					<imageheight>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.imageheight</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</imageheight>
					<border>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.border</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</border>
					<duration>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.duration</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</duration>
					<perspective>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.perspective</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</perspective>
					<minScale>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.minScale</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jftcaforms/lib/class.tx_jftcaforms_tceFunc.php:&tx_jftcaforms_tceFunc->getExtSlider]]></userFunc>
								<width>300</width>
								<range>
									<lower>0</lower>
									<upper>1</upper>
								</range>
								<decimalPrecision>2</decimalPrecision>
								<emptyValue>1</emptyValue>
							</config>
						</TCEforms>
					</minScale>
					<loadingClass>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.loadingClass</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</loadingClass>
				</el>
			</ROOT>
		</settings>
		<special>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.sheet_special</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<options>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jfspacegallery/locallang_db.xml:tt_content.pi_flexform.options</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>10</rows>
							</config>
						</TCEforms>
					</options>
				</el>
			</ROOT>
		</special>
	</sheets>
</T3DataStructure>

